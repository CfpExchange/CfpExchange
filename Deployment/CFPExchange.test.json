{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourcegroup": {
      "value": "cfpexchange-jan-test"
    },
    "hostingPlanName": {
      "value": "cfpexchange-test"
    },
    "administratorLogin": {
      "value": "cfpexchangeadministrator"
    },
    "administratorLoginPassword": {
      "value": "SomePasswordToBeFilledOutByYourFavouriteDeploymentTool"
    },
    "sqlServerName": {
      "value": "cfpexchange-test"
    },
    "databaseName": {
      "value": "cfpexchange-test"
    },
    "websiteName": {
      "value": "cfpexchange-test"
    },
    "AD.TenantId": {
      "value": "YourTenantId"
    },
    "AD.ObjectId": {
      "value": "YourAADObjectID"
    },
    "servicebusNamespace": {
      "value": "cfpexchange-bus-test"
    },
    "serviceBusQueueName": {
      "value": "eventimages-test"
    },
    "vaultName": {
      "value": "cfpexchange-vault"
    },
    "servicebusQueueConnectionString": {
      "reference": {
        "keyVault": {
          "id": "[resourceId(subscription().subscriptionId,  parameters('resourcegroup'), 'Microsoft.KeyVault/vaults', parameters('vaultName'))]"
        },
        "secretName": "ServicebusQueueConnectionString"
      }
    },
    "FeatureToggle.HostOwnImages": {
      "value": true
    }
  }
}